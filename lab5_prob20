#include <iostream>
using namespace std;

int main() {
    for (int limit = 10000; limit <= 100000; limit += 10000) {
        double pi = 0.0;
        for (int i = 1; i <= limit; i++) {
            if (i % 2 == 0) pi -= 4.0 / (2 * i - 1);
            else pi += 4.0 / (2 * i - 1);
        }
        cout << "i = " << limit << " Pi â‰ˆ " << pi << endl;
    }
    return 0;
}
