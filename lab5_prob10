#include <iostream>
using namespace std;

int main() {
    int num;
    cout << "Enter a number: ";
    cin >> num;
    int highest;
    highest = num;
    while (true) {
        if (num==0) {
            break;
        }
        cout<<"Enter an another number: ";
        cin>>num;
        if (num==0) {
            break;
        }
        if (num>highest) {
            highest = num;
        }
    }
    cout<<"The highest number is: "<<highest<<endl;
}
